<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.video.bestofnhk" name="Best of NHK" version="0.9.11" provider-name="misty">
  <requires>
        <import addon="xbmc.python" version="2.1.0"/>
	    <import addon="script.module.t0mm0.common" version="2.1.1"/>
	    <import addon="script.module.simple.downloader" version="1.9.4"/>
	    <import addon="script.video.F4mProxy" version="2.1.3"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
  	    <platform>all</platform>
  	    <language></language>
        <summary>bestofnhk.tv</summary>
        <description>Weird, fun, interesting documentaries from NHK World Japan. New shows daily.</description>
  </extension>
</addon>

<addon id="repository.misty" name="misty's Addons" version="1.0.0" provider-name="misty">
	<extension point="xbmc.addon.repository" name="misty's Addon Repository">
		<info compressed="false">https://github.com/misty-/addons/raw/master/addons.xml</info>
		<checksum>https://github.com/misty-/addons/raw/master/addons.xml.md5</checksum>
		<datadir zip="true">https://github.com/misty-/addons/raw/master/repo/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>misty's addons</summary>
		<description>Download and install addons by misty</description>
		<platform>all</platform>
	</extension>
</addon>
</addons>
